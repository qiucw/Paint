package model;

import java.util.ArrayList;

/**
 * Class for the model of Shape List.
 * 
 * @author Chenwei Qiu
 * @version June 10, 2015
 */
public class ArtModel {

    /**
     * field for List of Shape.
     */
    private ArrayList<ShapewProperties> myShapeList;
    
    /**
     * construct a art model.
     */
    public ArtModel() {
        myShapeList = new ArrayList<ShapewProperties>();
    }

    /**
     * getter for the list.
     * @return the list of shape.
     */
    public ArrayList<ShapewProperties> getList() {
        return myShapeList;
    }
}
